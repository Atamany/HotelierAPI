@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("/Views/AdminLayout/_AdminLayoutHead.cshtml")
<body>
    @await Html.PartialAsync("/Views/AdminLayout/_AdminLayoutPreLoader.cshtml")
    <div id="main-wrapper">
        @await Html.PartialAsync("/Views/AdminLayout/_AdminLayoutNavHeader.cshtml")
        @await Html.PartialAsync("/Views/AdminLayout/_AdminLayoutHeader.cshtml")
        @await Html.PartialAsync("/Views/AdminLayout/_AdminLayoutSidebar.cshtml")
        <div class="content-body">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                <p>@RenderBody()</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @await Html.PartialAsync("/Views/AdminLayout/_AdminLayoutFooter.cshtml")
    </div>
    @await Html.PartialAsync("/Views/AdminLayout/_AdminLayoutScript.cshtml")
</body>
</html>